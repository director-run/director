{
  "name": "@workingdev/codex",
  "author": "working.dev",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run build --watch",
    "build": "rm -rf dist/ && tsup",
    "lint": "biome check .",
    "format": "biome format --write .",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "postinstall": "bun run build",
    "teest": "bun run src/playground.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./parsers": {
      "types": "./dist/parsers/index.d.ts",
      "import": "./dist/parsers/index.js",
      "require": "./dist/parsers/index.cjs"
    },
    "./sources": {
      "types": "./dist/sources/index.d.ts",
      "import": "./dist/sources/index.js",
      "require": "./dist/sources/index.cjs"
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.9",
    "@lancedb/lancedb": "^0.16.0",
    "@tree-sitter-grammars/tree-sitter-markdown": "^0.3.2",
    "ai": "^4.1.25",
    "apache-arrow": "^19.0.0",
    "chokidar": "^4.0.3",
    "fast-glob": "^3.3.3",
    "gpt-tokenizer": "^2.8.1",
    "minimatch": "^10.0.1",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-json": "^0.24.8",
    "tree-sitter-typescript": "^0.23.2",
    "tree-sitter-wasms": "^0.1.12",
    "web-tree-sitter": "^0.25.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.2.0",
    "@types/json-to-ast": "^2.1.4",
    "@types/node": "^22.10.10",
    "tsup": "^8.3.5",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
