{
  "name": "@director.run/registry",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "exports": {
    "./*": "./src/*.ts"
  },
  "bin": {
    "registry": "./dist/cli.js"
  },
  "scripts": {
    "lint": "biome check .",
    "format": "biome format --write .",
    "clean": "rm -rf node_modules .turbo dist/*",
    "typecheck": "tsc --noEmit",
    "test": "bun run vitest",
    "cli": "bun run bin/cli.ts",
    "build": "bun build --production --target=node bin/cli.ts --outdir dist",
    "prepare": "bun run build",
    "db:push": "drizzle-kit push"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.14",
    "@director.run/registry-client": "workspace:*",
    "drizzle-kit": "^0.31.1",
    "type-fest": "^4.40.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "drizzle-orm": "^0.43.1",
    "next": "^15.3.2",
    "pg": "^8.15.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "commander": "^13.1.0"
  }
}
