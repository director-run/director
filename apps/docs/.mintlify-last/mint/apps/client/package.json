    "dev-watch": "next-remote-watch ./src/_props ./public",
    "export": "next build && next export",
    "format": "prettier \"**/*.{css,js,ts,tsx,md,mdx}\" --write",
    "format:check": "prettier \"**/*.{css,js,ts,tsx,md,mdx}\" --check",
    "lint": "eslint . --cache",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "zip": "yarn pack --install-if-needed --out mint-%v.tgz"
