{
  "name": "@workingco/icons",
  "author": "working.dev",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun run build:icons && bun run build:pkg",
    "build:pkg": "tsup",
    "build:icons": "bun run scripts/generate-icons.ts",
    "lint": "biome check . --error-on-warnings",
    "format": "biome format --write .",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "postinstall": "bun run build",
    "test": "echo 'No tests yet for icons package'"
  },
  "peerDependencies": {
    "react-dom": "^19.0.0",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.2.0",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
