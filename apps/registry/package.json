{
  "name": "@director.run/registry",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "bin": {
    "registry": "./dist/cli.js"
  },
  "scripts": {
    "lint": "biome check .",
    "format": "biome format --write .",
    "clean": "rm -rf node_modules .turbo dist/*",
    "typecheck": "tsc --noEmit",
    "test": "bun run vitest",
    "cli": "bun run bin/cli.ts",
    "build": "bun build --production --target=node bin/cli.ts --outdir dist",
    "prepare": "bun run build",
    "db:push": "drizzle-kit push"
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.2.13",
    "@types/pg": "^8.15.2",
    "@director.run/utilities": "workspace:*",
    "drizzle-kit": "^0.31.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "@faker-js/faker": "^9.8.0"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "drizzle-orm": "^0.43.1",
    "pg": "^8.16.0"
  }
}
