{
  "ssePort": 3006,
  "proxies": [
    {
      "name": "my-first-proxy",
      "servers": [
        {
          "name": "Hackernews",
          "transport": {
            "command": "uvx",
            "args": ["--from", "git+https://github.com/erithwik/mcp-hn", "mcp-hn"]
          }
        },
        {
          "name": "Fetch",
          "transport": {
            "command": "uvx",
            "args": ["mcp-server-fetch"]
          }
        }
      ]
    }
  ]
}
